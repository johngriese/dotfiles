! .Xresources
!
! loads modules from .Xresources.d
#include ".Xresources.d/rxvt-unicode"
#include ".Xresources.d/colors.d/gruvbox.dark"
!#include ".Xresources.d/colors.d/solarized.dark"

! Global Settings
!
! diasbles scrollbar
*.scrollBar:		false

!! Fonts
!
! URxvt has its own declarations for glyph support
! see .Xresources.d/rxvt-unicode
!

*.font:             xft:Misc Termsyn:regular:size=10, \
                    xft:Misc Siji:regular:size=10
*.boldFont:         xft:Misc Termsyn:regular:size=10, \
                    xft:Misc Siji:regular:size=10
! Font Settings
!
Xft.dpi:		    96
Xft.antialias:		true
Xft.hinting:		true
Xft.rgba:		    rgb
Xft.autohint:		false
Xft.hintstyle:		hintstyle
Xft.lcdfilter:		lcddefault
